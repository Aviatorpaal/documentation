{
"ignorePatterns": [
    {
      "pattern": "^ixsystems.com"
    },
    {
      "pattern": "^truecommand.io"
    }
  ],
"replacementPatterns": [
    {
      "pattern": "(?<=^\/(hub|hardware|truecommand)\/.+)\/$",
      "replacement": ".md"
    },
    {
      "pattern": "(?<=^\/(hub|hardware|truecommand)\/.+)\/#(?=.+)",
      "replacement": ".md#"
    },
    {
      "pattern": "^\/",
      "replacement": "{{BASEURL}}/content/en/"
    }
  ]
}
