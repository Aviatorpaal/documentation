{
  "projectBaseUrl": "content/en",
  "replacementPatterns": [
    {
      "pattern": "^/hub/",
      "replacement": "/content/en/hub/"
    }
    {
      "pattern": "^/truecommand/",
      "replacement": "/content/en/truecommand/"
